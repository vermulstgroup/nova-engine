{
  "name": "@nova-engine/msteams",
  "version": "2026.2.16",
  "description": "Nova Engine Microsoft Teams channel plugin",
  "type": "module",
  "dependencies": {
    "@microsoft/agents-hosting": "^1.2.3",
    "@microsoft/agents-hosting-express": "^1.2.3",
    "@microsoft/agents-hosting-extensions-teams": "^1.2.3",
    "express": "^5.2.1"
  },
  "devDependencies": {
    "nova-engine": "workspace:*"
  },
  "nova-engine": {
    "extensions": [
      "./index.ts"
    ],
    "channel": {
      "id": "msteams",
      "label": "Microsoft Teams",
      "selectionLabel": "Microsoft Teams (Bot Framework)",
      "docsPath": "/channels/msteams",
      "docsLabel": "msteams",
      "blurb": "Bot Framework; enterprise support.",
      "aliases": [
        "teams"
      ],
      "order": 60
    },
    "install": {
      "npmSpec": "@nova-engine/msteams",
      "localPath": "extensions/msteams",
      "defaultChoice": "npm"
    }
  }
}
